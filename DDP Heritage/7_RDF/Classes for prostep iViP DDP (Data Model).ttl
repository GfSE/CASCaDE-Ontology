#################################################################
### META-ONTOLOGY = PIG Metamodel (pig:)
#################################################################

pig:OrganizerClass
	rdf:type owl:Class ;
	rdfs:label "Organizer Class" ;
	rdfs:comment "A class of organizers." .

pig:Organizer
	rdf:type owl:Class ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Organizer" ;
	rdfs:comment "An organizer." .

pig:EntityClass
	rdf:type owl:Class ;
	rdfs:label "Entity Class" ;
	rdfs:comment "A class of entities." .

pig:Entity
	rdf:type owl:Class ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Entity" ;
	rdfs:comment "An entity." .

pig:RelationshipClass
	rdf:type owl:Class ;
	rdfs:label "Relationship Class" ;
	rdfs:comment "A class of relationship resources." .

pig:Relationship
	rdf:type owl:Class ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Relationship" ;
	rdfs:comment "A relationship resource." .

pig:hasSubject
	rdf:type owl:ObjectProperty ;
	rdfs:domain pig:Relationship ;
	rdfs:range pig:Entity ;
	rdfs:label "has subject" ;
	rdfs:comment "Connects the subject of a relationship." .

pig:hasObject
	rdf:type owl:ObjectProperty ;
	rdfs:domain pig:Relationship ;
	rdfs:range pig:Entity ;
	rdfs:label "has object" ;
	rdfs:comment "Connects the object of a relationship." .

